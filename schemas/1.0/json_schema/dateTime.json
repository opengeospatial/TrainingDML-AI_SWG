{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/opengeospatial/TrainingDML-AI_SWG/main/schemas/1.0/json_schema/dateTime.json",
    "title": "DateTime",
    "oneOf": [
        {
            "type": "string",
            "format": "date-time"
        },
        {
            "type": "string",
            "format": "date"
        },
        {
            "type": "string",
            "format": "time"
        },
        {
            "type": "string",
            "pattern": "^(19|20)\\d{2}$"
        },
        {
            "type": "string",
            "pattern": "^(19|20)\\d{2}-(0[1-9]|1[0-2])$"
        }
    ]
}