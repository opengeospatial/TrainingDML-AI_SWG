{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/opengeospatial/TrainingDML-AI_SWG/main/schemas/1.0/json_schema/ai_metricsInLiterature.json",
    "title": "AI_MetricsInLiterature",
    "type": "object",
    "required": [
        "doi",
        "metrics"
    ],
    "properties": {
        "algorithm": {
            "type": "string"
        },
        "doi": {
            "type": "string"
        },
        "metrics": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "name",
                    "value"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "value": {
                        "type": "number"
                    }
                }
            },
            "minItems": 1
        }
    }
}