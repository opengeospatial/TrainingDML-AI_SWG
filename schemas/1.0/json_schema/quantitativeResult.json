{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/opengeospatial/TrainingDML-AI_SWG/main/schemas/1.0/json_schema/quantitativeResult.json",
    "title": "QuantitativeResult",
    "type": "object",
    "required": [
        "value"
    ],
    "properties": {
        "value": {
            "type": "array",
            "items": {
                "$comment": "The type of the value is 'any'.",
                "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "object"
                    },
                    {
                        "type": "number"
                    },
                    {
                        "type": "integer"
                    },
                    {
                        "type": "array"
                    },
                    {
                        "type": "boolean"
                    },
                    {
                        "type": "null"
                    }
                ]
            }
        },
        "valueUnit": {
            "type": "string"
        },
        "valueRecordType": {
            "type": "string"
        }
    }
}